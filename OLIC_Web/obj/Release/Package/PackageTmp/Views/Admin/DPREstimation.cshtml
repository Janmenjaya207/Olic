

@{
    /**/

    /**/

    Layout = null;
}


<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">

    <title>OLIC</title>
    <meta content="" name="description">
    <meta content="" name="keywords">

    <!-- Favicons -->
    <link href="~/dprassets/img/favicon.png" rel="icon">
    <link href="~/dprassets/img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Roboto:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

    <!-- Vendor CSS Files -->
    <link href="~/dprassets/vendor/aos/aos.css" rel="stylesheet">
    <link href="~/dprassets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/dprassets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="~/dprassets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="~/dprassets/vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
    <link href="~/dprassets/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">

    <!-- Template Main CSS File -->
    <link href="~/dprassets/css/style.css" rel="stylesheet">


    <style type="text/css">
        .container2 {
            padding: 0px 145px;
        }
    </style>
</head>

<body>

    <section>
        <div class="container">
            <div class="row">
                <div id="options">
                    <input id="printpagebutton" type="button" class="btn btn-success" style="position: absolute; top: 20px; font-weight: bold; font-size: large; right: 220px;" value="Print" onclick="printpage()" />
                </div>
                <div id="options">
                    <a href="/Admin/DBW_DPR_Master">
                        <input id="Backpagebutton" type="button" class="btn btn-danger" style="position: absolute; top: 20px; font-weight: bold; font-size: large; right: 140px;" value="Back" onclick="Backpage()" />
                    </a>
                </div>
            </div>
            <h5 style="text-align: center;margin-bottom: 12px"><b>Rate chart for preparation of Estimate of deep bore wells</b></h5>
            <table class="table table-bordered">
                <thead style="text-align: center;">
                    <tr>
                        <th>
                            Sl.No.
                        </th>
                        <th>
                            Description of work
                        </th>
                        <th>
                            Unit
                        </th>
                        <th>
                            Rate per Unit
                        </th>
                        <th>
                            Remarks
                        </th>
                    </tr>
                </thead>
                <tbody>

                    @{ int count = 0;}
                    @if (ViewBag.DprDtl != null)
                    {
                        foreach (var iteam in ViewBag.DprDtl)
                        {

                            count = count + 1;

                            <tr role="row" class="even">

                                <td>@count</td>
                                <td>@iteam.Description_of_work</td>
                                <td>@iteam.Unit</td>
                                <td>@iteam.Rate_per_Unit</td>
                                <td>@iteam.Remarks</td>
                            </tr>
                        }
                    }

                    <tr>
                        <td colspan="2" style="text-align: right;font-weight: 600;">
                            TOTAL
                        </td>
                        <td></td>
                        <td>@ViewBag.a1</td>
                        <td></td>
                    </tr>
                    <tr>
                        <td colspan="2" style="text-align: right;">
                            Misc. 8.5%
                        </td>
                        <td></td>
                        <td>@ViewBag.a2</td>
                        <td></td>
                    </tr>
                    <tr>
                        <td colspan="2" style="text-align: right;font-weight: 600;">
                            TOTAL
                        </td>
                        <td></td>
                        <td>@ViewBag.a3</td>
                        <td></td>
                    </tr>
                    <tr>
                        <td colspan="2" style="text-align: right;">
                            GST 12%
                        </td>
                        <td></td>
                        <td>@ViewBag.a4</td>
                        <td></td>
                    </tr>
                    <tr>
                        <td colspan="2" style="text-align: right;font-weight: 600;">
                            G. TOTAL
                        </td>
                        <td></td>
                        <td>@ViewBag.a5</td>
                        <td></td>
                    </tr>
                    <tr>
                        <td colspan="2" style="text-align: center;font-weight: 600;">
                            Hence Average cost of one Bore well
                        </td>
                        <td></td>
                        <td><b>@ViewBag.a6</b><b>.00</b></td>
                        <td></td>
                    </tr>
                </tbody>
            </table>
        </div>

    </section>







    <!-- ======= Footer ======= -->

    <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>


</body>

</html>
<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
<script>
    $(document).ready(function () {

        $("#btnExport").click(function () {
            var mode = 'iframe'; // popup
            var close = mode == "popup";
            var options = { mode: mode, popClose: close };
            $("div.bg1").printArea(options);
        });
        $(document).bind("contextmenu", function (e) {
            return false;
        });
    });
    $(document).on('keydown', function (e) {
        if ((e.ctrlKey || e.metaKey) && (e.key == "p" || e.charCode == 16 || e.charCode == 112 || e.keyCode == 80)) {
            alert("Please use the Print button for a better rendering on the document");
            e.cancelBubble = true;
            e.preventDefault();
            e.stopImmediatePropagation();
        }
    });
</script>
<script>
    function printpage() {
        //Get the print button and put it into a variable
        var printButton = document.getElementById("printpagebutton");
        var BackpageButton = document.getElementById("Backpagebutton");
        //Set the print button visibility to 'hidden'
        printButton.style.visibility = 'hidden';
        BackpageButton.style.visibility = 'hidden';
        //Print the page content
        window.print()
        printButton.style.visibility = 'visible';
        BackpageButton.style.visibility = 'visible';
    }
    
</script>
<script type="text/javascript">
    var isNS = (navigator.appName == "Netscape") ? 1 : 0;

    if (navigator.appName == "Netscape") document.captureEvents(Event.MOUSEDOWN || Event.MOUSEUP);

    function mischandler() {
        return false;
    }

    function mousehandler(e) {
        var myevent = (isNS) ? e : event;
        var eventbutton = (isNS) ? myevent.which : myevent.button;
        if ((eventbutton == 2) || (eventbutton == 3)) return false;
    }
    document.oncontextmenu = mischandler;
    document.onmousedown = mousehandler;
    document.onmouseup = mousehandler;
    var isCtrl = false;
    document.onkeyup = function (e) {
        if (e.which == 17)
            isCtrl = false;
    }

    document.onkeydown = function (e) {
        if (e.which == 17)
            isCtrl = true;
        if (((e.which == 85) || (e.which == 117) || (e.which == 65) || (e.which == 97) || (e.which == 67) || (e.which == 99)) && isCtrl == true) {
            // alert(‘Keyboard shortcuts are cool!’);
            return false;
        }
    }
</script>