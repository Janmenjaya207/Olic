
@{
    ViewBag.Title = "Total_paid_application_deep_borewell";
    Layout = "~/Views/Shared/_CommonLayout.cshtml";
}


<div class="card">
    <div class="card-body">
        <div class="table-responsive mb-3" style="margin-top: 40px;">
            <h4 style="margin-bottom: inherit;">Total Paid Farmer:</h4>
            <table id="example2" class="table table-striped table-bordered">
                <thead>
                    <tr>
                        <th>Sl. No.</th>
                        <th>Registration No</th>
                        <th>Name</th>
                        <th>Gender</th>
                        <th>Mobile No</th>
                        <th>Age</th>
                        <th>Aadhaar No</th>
                        <th>District</th>
                        <th>Block</th>
                        <th>Village</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>

                    @{ int count = 0;}
                    @if (ViewBag.PaidDeepBorewell != null)
                    {
                        foreach (var iteam in ViewBag.PaidDeepBorewell)
                        {

                            count = count + 1;

                    <tr role="row" class="even">

                        <td>@count</td>
                        <td>@iteam.DbRegistrationNo</td>
                        <td>@iteam.FarmerName</td>
                        <td>@iteam.MobileNo</td>
                        <td>@iteam.Gender</td>
                        <td>@iteam.Category</td>
                        <td>@iteam.AadhaarNo</td>
                        <td>@iteam.DistrictName</td>
                        <td>@iteam.Block_Ulb_Name</td>
                        <td>@iteam.GRAM_PANCHAYAT_Name</td>
                        <td>
                            <a href="/Finance/ShowRegistationdtls?id=@iteam.DeepBorewellRegdId" class="btn btn-outline-primary waves-effect waves-light m-1 btn1 btn-sm" title="View"><i class="fa fa-eye"></i></a>
                        </td>
                    </tr>


                        }
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>