    <div class="card">
        <div class="card-body">
            <h6 class="mb-0 text-uppercase">Vendor Agreement</h6>
            <hr>
            <div class="row mb-3">
                <div class="col-md-3">
                    <label for="validationCustom04" class="form-label">Name of the Vendor:</label>
                    <select class="form-select" id="validationCustom04" required="">
                        <option value="1">Select</option>
                        <option value="1">Vendor 1</option>
                        <option value="2">Vendor 2</option>
                        <option value="3">Vendor 3</option>
                    </select>
                </div>
                <div class="col-md-3">
                    <label for="validationCustom04" class="form-label">Package Name:</label>
                    <input type="text" class="form-control" disabled id="PackageName" value="CTC-PKG-0001" />
                </div>
                <div class="col-md-3">
                    <label for="validationCustom04" class="form-label">Agreement No:</label>
                    <input type="text" class="form-control" id="AgreementNo" />
                </div>
                <div class="col-md-3">
                    <label for="validationCustom04" class="form-label">Agreement Date:</label>
                    <input type="date" class="form-control" id="FromDate" />
                </div>
            </div>
            <div class="row mb-3">
                <div class="col-md-3">
                    <label for="validationCustom04" class="form-label">Upload Agreement File:</label>
                    <input type="file" class="form-control" id="AgreeFile" />
                </div>
                <div class="col-md-3">
                    <label for="validationCustom04" class="form-label">Remarks:</label>
                    <textarea class="form-control" id="Remarks"></textarea>
                </div>
            </div>
            @*<div class="row mb-3">
